{
  "name": "my-accountability-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "docker:build": "docker build -t accountability-app .",
    "docker:run": "docker run -p 3000:3000 accountability-app",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "type-check": "tsc --noEmit",
    "db:setup": "bash scripts/setup-supabase.sh",
    "db:migrate": "echo 'Run supabase/migrations/001_initial_schema.sql in Supabase SQL Editor'",
    "db:seed-exercises": "echo 'Run scripts/seed-exercises.sql in Supabase SQL Editor'",
    "db:seed-data": "echo 'Run scripts/seed-sample-data.sql in Supabase SQL Editor'",
    "db:help": "cat scripts/SUPABASE_SETUP_GUIDE.md"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-pwa": "^5.6.0",
    "openai": "^5.23.1",
    "react": "19.1.0",
    "react-circular-progressbar": "^2.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
